import java.util.*;
import java.lang.Math;

public class JavaApplication {
    private static Scanner input = new Scanner(System.in);

    public static void main(String[] args) {
      
    	//
    	System.out.println( "PROGRAMA PARA TROCA DE VALORES ENTRE VARIÁVEIS");
    	
    	int n1;
        System.out.println("Digite o primeiro número: ");
        n1 = input.nextInt();
        
        int n2;       
        System.out.println("Digite o segundo número: ");
        n2 = input.nextInt();
        
        System.out.println("O valor atual do primeiro número é: " + n1 ); 
        System.out.println("O valor atual do segundo número é: " + n2 + "\n");
       
      // PROCESSAMENTO
        int troca;
        
        troca = n1;
        n1 = n2;
        n2 = troca;
      
        System.out.println("O novo valor do primeiro número é: "  + n1 +"\n"+ "O novo valor do segundo  número é: " + n2);
    }
}
